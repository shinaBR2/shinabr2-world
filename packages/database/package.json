{
  "name": "database",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "postinstall": "pnpx prisma generate",
    "build": "tsup src/index.ts --format esm,cjs && mkdir -p dist/generated/client && cp -r src/generated/client/ dist/generated/client/",
    "db:generate": "pnpx prisma generate",
    "db:push": "prisma db push --skip-generate",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@prisma/client": "latest"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "tsconfig": "workspace:*",
    "prisma": "latest",
    "typescript": "^4.8.3",
    "tsup": "^8.0.0"
  }
}